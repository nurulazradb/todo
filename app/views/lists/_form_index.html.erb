<%= form_with(model: list, local: true, class: 'form-inline my-2 my-lg-0') do |form| %>
  <%= form.text_field :description, id: :list_description, class: 'form-control mr-sm-2', placeholder: 'Todo item' %>
  <%= form.submit 'Add Todo Item', class: 'btn btn-outline-success my-2 my-sm-0' %>
<% end %>
